<%= form_with model: @goal do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :completion_date %>
  <%= f.text_field :completion_date %>
  <%= f.label :review %>
  <%= f.text_field :review %>
  <%= f.submit %>
<% end %>
