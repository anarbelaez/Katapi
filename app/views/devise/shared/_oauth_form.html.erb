<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= form_for "Login",
      url: omniauth_authorize_path(resource_name, provider),
      method: :post,
      class: "",
      data: { turbo: "false" } do |f| %>
        <%= f.submit "Login", type: "image", src: "https://upload.wikimedia.org/wikipedia/commons/thumb/5/53/Google_%22G%22_Logo.svg/1200px-Google_%22G%22_Logo.svg.png", style: "width: 25px" %>
    <% end %>
  <% end %>
<% end %>
